{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": [1, 4],
        "template-curly-spacing": [1, "always"],
        "no-use-before-define": [0],
        "comma-dangle": [1, "never"]
    }
}
